    <property>
      <code>/*
 * Reduce a little the paddings, to make some more room on the page.
*/
div.kanban-item {
  padding: @grid-gutter-width / 3;
}

div.kanban-board .kanban-drag {
  padding: @grid-gutter-width / 3;
}

div.kanban-board {
  margin-left: 0.5% !important;
  margin-right: 0.5% !important;
}

/*
 * Arrange the cards for the items
 */
/* 1. remove padding for the items list */
ul.card-field-list {
  margin-left: 0;
  padding-left: 0;
  list-style-position: inside;
}

/* 2. no bold on the item titles, smaller fonts, no margin on the entry title and bold title */
.kanban-item {
  .kanban-item-content {
    font-size: 0.85em;
    opacity: 0.75;
    .card-field-title {
      font-weight: normal;
    }
  }

  .kanban-item-title {
    font-weight: bold;
    margin: 0 0 10px 0;
  }
}
</code>
